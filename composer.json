{
  "name": "greenskies/json",
  "type": "library",
  "license": "MIT",
  "description": "An improved php json library",
  "autoload": {
    "psr-4": {
      "Greenskies\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "config": {
    "sort-packages": true,
    "bin-dir": "bin"
  },
  "require": {
    "justinrainbow/json-schema": "^5.2",
    "karriere/json-decoder": "^2.2",
    "symfony/options-resolver": "^4.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0"
  }
}
