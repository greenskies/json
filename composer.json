{
  "name": "greenskies/json",
  "type": "library",
  "license": "MIT",
  "description": "An improved php json library",
  "authors": [
    {
      "name": "Johannes Pichler",
      "email": "johannes.pichler@karriere.at",
      "role": "Maintainer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Greenskies\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "config": {
    "sort-packages": true,
    "bin-dir": "bin"
  },
  "require": {
    "ext-json": "*",
    "justinrainbow/json-schema": "^5.2",
    "karriere/json-decoder": "^2.2",
    "symfony/options-resolver": "^4.2"
  },
  "require-dev": {
    "php-coveralls/php-coveralls": "^2.1",
    "phpunit/phpunit": "^7.0 | ^8.0"
  }
}
